{
  "name": "ensi/b2cpl-sdk-php",
  "description": "Integration (SDK) with B2Cpl API",
  "keywords": [
    "B2Cpl",
    "sdk",
    "integration",
    "REST"
  ],
  "minimum-stability": "dev",
  "type": "library",
  "authors": [
    {
      "name": "Evgeny Bereza",
      "email": "bereza@greensight.ru"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "suggests": {
    "guzzlehttp/guzzle": "Needed if you want to use GuzzleAdapter"
  },
  "autoload-dev": {
    "psr-4": {
      "Ensi\\B2Cpl\\Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "Ensi\\B2Cpl\\": "src"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
